msgid "Original"
msgstr "Original"

msgctxt "The accessibility label of the video resize dropdown for screen readers."
msgid "Video resize list"
msgstr "Video resize list"

msgctxt "The label of the form submit button if the video source URL input has no value."
msgid "Insert"
msgstr "Insert"

msgctxt "The label of the form submit button if the video source URL input has a value."
msgid "Update"
msgstr "Update"

msgctxt "The input label for the Insert video via URL form."
msgid "Insert video via URL"
msgstr "Insert video via URL"

msgctxt "The input label for the Insert video via URL form for a pre-existing video."
msgid "Update video URL"
msgstr "Update video URL"

